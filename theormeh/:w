#include <stdlib.h>
#include <stdio.h>

#include "exp.h"

double* exp_(double t, double* x)
{
  double l_0 = 1;
  double omega = 1;

  double* x_diff = (double*) calloc(2, sizeof(double));
  x_diff[0] = x[1];
  x_diff[1] = - omega * omega * (x[0] - l_0); //l_0 - lenght of a spring in unstreched condition

  return x_diff;
}
